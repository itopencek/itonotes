<body>
    <header>
        
    </header>
    <main>
        <section id='editor-wrapper'>
            <div id='editor'>
            </div>
        </section>
    </main>
    <footer>

    </footer>
    <script>
        var quill = new Quill('#editor', {
  modules: {
    toolbar: [
      [ { size: [ 'small', false, 'large', 'huge' ] } ],
      ['bold', 'italic', 'underline', 'strike'],
      [{'color' : [] },{'background' : [] }],
      ['link']
    ]
  },
  placeholder: 'Note everything...',
  theme: 'snow'  // or 'bubble'
});
    </script>
</body>
